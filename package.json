{
    "name": "vue-composable-lightbox",
    "version": "0.1.0",
    "description": "Simple vue composable wrapper for photoswipe lightbox",
    "main": "dist/vue-composable-lightbox.umd.js",
    "module": "dist/vue-composable-lightbox.es.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "import": "./dist/vue-composable-lightbox.es.js",
            "require": "./dist/vue-composable-lightbox.umd.js",
            "types": "./dist/index.d.ts"
        }
    },
    "scripts": {
        "build": "vite build"
    },
    "peerDependencies": {
        "photoswipe": "^5.4.4",
        "vue": "^3.5.13"
    },
    "devDependencies": {
        "@vitejs/plugin-vue": "^5.2.3",
        "@vue/tsconfig": "^0.7.0",
        "typescript": "~5.8.3",
        "vite": "^6.3.5",
        "vite-plugin-dts": "^4.5.3",
        "vue-tsc": "^2.2.8"
    },
    "keywords": [
        "vue",
        "vue3",
        "composable",
        "photoswipe",
        "lightbox",
        "plugin"
    ],
    "author": "Johannes Arvidsson",
